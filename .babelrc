{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": ["lodash"],
  "env": {
    "production": {
      "plugins": [
        "transform-react-inline-elements",
        "transform-react-constant-elements"
      ]
    },
    "development": {
      "presets": ["react-hmre"],
      "plugins": [
        "react-hot-loader/babel",
        "transform-runtime",
      ]
    },
  }
}
